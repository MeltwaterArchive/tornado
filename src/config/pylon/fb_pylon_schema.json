            "perms": [],
            "is_mandatory": true
        },
        {
            "target": "fb.author.age",
            "is_analysable": true,
            "is_filterable": true,
            "requires_threshold": false,
            "cardinality": 7,
            "description": "One of '18-24', '25-34', '35-44', '45-54', '55-64', '65+' or 'unknown' (No < 18 ages allowed even in unknown; rows from such actors will be filtered)."
        },
