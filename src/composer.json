{
    "name": "datasift/ms-app-tornado",
    "description": "Tornado",
    "license": "proprietary",
    "authors": [
        {
            "name": "Christopher Hoult",
            "email": "christopher.hoult@datasift.com"
        },
        {
            "name": "Michael Heap",
            "email": "michael.heap@datasift.com"
        }
    ],
    "require": {
        "silex/silex": "~1.3",
        "twig/twig": "1.18.*",
        "symfony/dependency-injection": "^2.7",
        "symfony/config": "2.7.*",
        "symfony/yaml": "2.7.*",
        "symfony/twig-bridge": "2.7.*",
        "symfony/console": "2.7.*",
        "symfony/finder": "~2.3|3.0.*",
        "symfony/validator": "~2.3|3.0.*",
        "doctrine/dbal": "2.5.*",
        "monolog/monolog": "1.14.*",
        "willdurand/negotiation": "1.3.*",
        "michaldudek/foundation": ">=0.9,<2.0",
        "datasift/datasift-php": "2.5.*",
        "firebase/php-jwt": "3.0.*",
        "mheap/Silex-Memcache": "1.0.0",
        "swiftmailer/swiftmailer": "5.4.1",
        "doctrine/migrations": "1.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*",
        "squizlabs/php_codesniffer": "2.3.*",
        "phpmd/phpmd": "2.2.*",
        "sebastian/phpcpd": "2.0.*",
        "phploc/phploc": "2.0.*",
        "phpdocumentor/phpdocumentor": "2.8.3",
        "pdepend/pdepend": "2.1.*",
        "behat/behat": "3.*",
        "nikic/php-parser": "1.3.*",
        "bartlett/php-compatinfo": "4.3.*",
        "datasift/testrest-extension": "3.0.0",
        "johnkary/phpunit-speedtrap": "~1.0.1",
        "fabpot/goutte": "~1.0.4",
        "behat/mink-extension": "2.2.*",
        "behat/mink-goutte-driver": "1.2.*",
        "behat/mink-selenium2-driver": "1.3.*"
    },
    "autoload": {
        "psr-4": {
            "Controller\\": "app/Controller",
            "Command\\": "app/Command",
            "Tornado\\": "lib/Tornado",
            "DataSift\\": "lib/DataSift"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Tornado\\": "../test/unit/src/lib/Tornado",
            "Test\\DataSift\\": "../test/unit/src/lib/DataSift"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}